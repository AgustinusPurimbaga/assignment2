<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#663300" />
            <gradient  android:angle="-90"  android:startColor="#664d00" android:endColor="#663300"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#663300" />
            <solid  android:color="#331a00"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#663300" />
            <gradient  android:angle="-90"  android:startColor="#331a00" android:endColor="#1a0d00" />
        </shape>
    </item>
</selector>